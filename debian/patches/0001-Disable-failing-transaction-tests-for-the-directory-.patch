+#	$(RUNENV) $(RUNCMD) ./kcdirtest order -th 4 -rnd -etc -tran casket 500
 	$(RUNENV) $(RUNCMD) ./kcdirmgr check -onr casket
-	$(RUNENV) $(RUNCMD) ./kcdirtest order -th 4 -rnd -etc -oat casket 500
+#	$(RUNENV) $(RUNCMD) ./kcdirtest order -th 4 -rnd -etc -oat casket 500
 	$(RUNENV) $(RUNCMD) ./kcdirmgr check -onr casket
